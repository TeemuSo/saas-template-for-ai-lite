{
    "mcpServers": {
      "supabase": {
        "command": "npx",
        "args": [
          "-y",
          "@supabase/mcp-server-supabase@latest"
        ],
        "env": {
            "SUPABASE_ACCESS_TOKEN": "YOUR_SUPABASE_ACCESS_TOKEN"
        }
      },
      "stripe": {
        "command": "npx",
        "args": [
          "-y",
          "@stripe/mcp",
          "--tools=all"
        ],
        "env": {
            "STRIPE_API_KEY": "YOUR_STRIPE_API_KEY"
        }
      },
      "context7": {
        "url": "https://mcp.context7.com/mcp"
      }
    }
}